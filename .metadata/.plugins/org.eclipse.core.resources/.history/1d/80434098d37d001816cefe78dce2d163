import java.io.*;
public class Inputnumbers {

	public static void main(String[] args)  throws IOException{
		// TODO 自動生成されたメソッド・スタブ
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		System.out.println("番号をスペース区切りで入力");
		String [] ss = br.readLine().split(" ");
		String swap = "";
		int horizon1 = 0;
		int horizon2 = 0;

		for (int i = 0; i<ss.length; i++) {

			for (int c = 1; c< ss.length-1; c++) {
//				if (c+1 == ss.length) {
//					continue;
//				}
				if(Integer.parseInt(ss[c]) > Integer.parseInt(ss[c+1])){
					swap = ss[c];
					ss[c] = ss[c+1];
					ss[c+1] =  swap;
				}else {
					continue;
				}

			}
//			for (String string : ss) {
//				System.out.println(string);
//			}
		}
		
		for(int i = 0; i<ss.length -2;i++) {
			if(Integer.parseInt(ss[i]) - Integer.parseInt(ss[i+1]) > Integer.parseInt(ss[i+1]) - Integer.parseInt(ss[i+2])) {
				horizon1 = i;
				horizon2 = i+1;
			}else {
				horizon1 = i+1;
				horizon2 = i+2;
			}



		System.out.println("答えは:" +ss[horizon1]+" "+ ss[horizon2]);


	}

}
